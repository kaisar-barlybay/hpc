{
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
    },
    "javascript.validate.enable": true,
    "[rmd]": {
        "editor.tabSize": 3
    },
    "emmet.includeLanguages": {
        "html.erb": "html",
        "django-html": "html"
    },
    "markdown-pdf.styles": [
        "markdown.css"
    ],
    "markdown-pdf.footerTemplate": "<div style=\"font-size: 9px; margin: 0 auto;\"> <span class='pageNumber'></span> / <span class='totalPages'></span></div>",
    "markdown-pdf.headerTemplate": "<div style=\"font-size: 9px; margin-left: 1cm;\"> </div> <div style=\"font-size: 9px; margin-left: auto; margin-right: 1cm; \"></div>",
    "editor.detectIndentation": false,
    "files.associations": {
        "*.erb": "erb",
        "django-html": "html",
        "*.js": "javascriptreact",
        "*.rmd": "markdown",
        "*.Rmd": "rmd",
        "algorithm": "c",
        "atomic": "c",
        "bit": "c",
        "cctype": "c",
        "clocale": "c",
        "cmath": "c",
        "compare": "c",
        "concepts": "c",
        "cstddef": "c",
        "cstdint": "c",
        "cstdio": "c",
        "cstdlib": "c",
        "cstring": "c",
        "ctime": "c",
        "cwchar": "c",
        "exception": "c",
        "fstream": "c",
        "initializer_list": "c",
        "ios": "c",
        "iosfwd": "c",
        "iostream": "c",
        "istream": "c",
        "iterator": "c",
        "limits": "c",
        "list": "c",
        "memory": "c",
        "mutex": "c",
        "new": "c",
        "ostream": "c",
        "random": "c",
        "ratio": "c",
        "stdexcept": "c",
        "stop_token": "c",
        "streambuf": "c",
        "string": "c",
        "system_error": "c",
        "thread": "c",
        "tuple": "c",
        "type_traits": "c",
        "typeinfo": "c",
        "unordered_map": "c",
        "unordered_set": "c",
        "utility": "c",
        "vector": "c",
        "xfacet": "c",
        "xhash": "c",
        "xiosbase": "c",
        "xlocale": "c",
        "xlocinfo": "c",
        "xlocnum": "c",
        "xmemory": "c",
        "xstddef": "c",
        "xstring": "c",
        "xtr1common": "c",
        "xutility": "c",
        "mmio.h": "c",
        "stdlib.h": "c",
        "stdio.h": "c",
        "timer.h": "c",
        "semaphore.h": "c",
        "pthread.h": "c"
    },
    "editor.suggestSelection": "first",
    "git.confirmSync": false,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.alwaysShowStatus": true,
    "files.autoSave": "onFocusChange",
    "diffEditor.wordWrap": "off",
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "explorer.confirmDelete": false,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.profiles.windows": {
        "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe (migrated)": {
            "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
            "args": []
        }
    },
    "editor.wordWrapColumn": 160,
    "eslint.format.enable": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "python.formatting.autopep8Args": [
        "--max-line-length=200",
        "--indent-size=2"
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "files.eol": "\n",
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "python.linting.mypyEnabled": false,
    "python.linting.mypyArgs": [
        "--follow-imports=silent",
        "--ignore-missing-imports",
        "--show-column-numbers",
        "--no-pretty"
    ],
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--indent-string='  '",
        "--disable=missing-function-docstring",
        "--disable=missing-module-docstring",
        "--disable=too-many-function-args",
        "--disable=missing-class-docstring",
        "--disable=line-too-long",
        "--disable=wrong-import-order",
        "--disable=invalid-name",
        "--disable=import-error",
        "--disable=no-member",
        "--disable=anomalous-backslash-in-string",
        "--disable=logging-fstring-interpolation",
        "--disable=too-many-ancestors",
        "--disable=unused-argument",
        "--disable=no-name-in-module",
        "--disable=too-many-instance-attributes",
        "--disable=too-few-public-methods",
        "--disable=dangerous-default-value",
        "--disable=no-self-use",
        "--disable=consider-using-with",
        "--disable=super-with-arguments",
        "--disable=unnecessary-lambda",
        "--disable=bare-except",
        "--disable=bad-classmethod-argument",
        "--disable=singleton-comparison",
        "--disable=too-many-arguments",
        "--disable=consider-using-f-string",
        "--disable=f-string-without-interpolation",
        "--disable=ungrouped-imports",
        "--disable=expression-not-assigned",
        "--disable=broad-except",
        "--disable=unused-variable",
        "--disable=too-many-locals",
        "--disable=attribute-defined-outside-init",
        "--disable=chained-comparison",
        "--disable=redefined-outer-name",
        "--disable=useless-super-delegation",
        "--disable=abstract-method",
        "--disable=import-outside-toplevel",
        "--disable=consider-using-enumerate",
        "--disable=unused-private-member",
        "--disable=unsubscriptable-object"
    ],
    "mypy.configFile": "mypy.ini",
    "mypy.runUsingActiveInterpreter": true,
    "cSpell.enabled": false,
    "cmake.configureOnOpen": false
}